{
  "name": "svelte-widget",
  "version": "0.0.1",
  "devDependencies": {
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "@pyoner/svelte-types": "^3.4.4-2",
    "fontello-cli": "^0.5.0",
    "npm-check-updates": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.1.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "sirv-cli": "^0.4.5",
    "svelte": "^3.20.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "upd-all": "npm outdated && ncu -u && npm update && npm install",
    "fontello": "fontello-cli install --config ./fontello.config.json --css ./public/assets/css --font ./public/assets/font"
  },
  "dependencies": {
    "prettier": "^1.19.1",
    "svelte-spinner": "^2.0.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
